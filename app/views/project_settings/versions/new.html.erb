<div class="project-settings new-version container">
    <h1 class="page-header"><%= t :new_version %></h1>

    <%= error_messages_for @version %>

    <%= form_for [:project_settings, @version], html: { class: 'form-horizontal' } do |f| %>
        <%= render 'form', f: f %>

        <div class="form-actions">
            <%= f.submit t(:create), class: 'btn btn-success' %>
        </div>
    <% end %>
</div>
