<div class="project_settings new_version content">
  <h2 id="page-title"><%=t :new_version %></h2>

  <%= form_for [:project_settings, @version], html: { class: 'form-horizontal' } do |f| %>
    <%= error_messages_panel_for @version %>
    <%= render partial: 'form', locals: { f: f } %>
    <div class="actions">
      <%= f.submit t(:create), class: 'btn btn-success' %>
    </div>
  <% end %>
</div>
