<div class="project-settings new-version container">
    <h1 class="page-header"><%= t :edit_version %></h1>

    <%= form_for @version, url: project_settings_version_path, html: { class: 'form-horizontal' } do |f| %>
        <%= render 'form', f: f %>

        <div class="form-actions">
            <%= f.submit t(:save), class: 'btn btn-success' %>
        </div>
    <% end %>
</div>
